{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        },
        "EndPoints": {
            "Http": {
                "Url": "http://*:10090"
            },
            "Https": {
                "Url": "https://*:10091"
            }
        },
        "Limits": {
            "MaxConcurrentConnections": 100,
            "MaxConcurrentUpgradedConnections": 100
        },
        "DisableStringReuse": true
    }
}
